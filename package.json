{
	"name": "ooxml-viewer",
	"displayName": "ooxml-viewer",
	"description": "Views contents of OOXML files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ooxmlViewer.viewContents",
		"onView:ooxmlViewer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ooxmlViewer.viewContents",
				"title": "View Contents",
				"when": "resourceLangId == ooxml"
			}
		],
		"languages": [
			{
				"id": "ooxml",
				"extensions": [
					".docx",
					".xlsx",
					".pptx"
				]
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "ooxmlViewer.viewContents",
					"group": "navigation",
					"when": "resourceLangId == ooxml"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "ooxmlViewer",
					"name": "OOXML Viewer"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jszip": "^3.1.6",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"jszip": "^3.2.2"
	}
}